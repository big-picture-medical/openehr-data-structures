{
    "name": "big-picture-medical/openehr-data-structures",
    "require": {
        "spatie/data-transfer-object": "^3.5",
        "symfony/finder": "^5.2",
        "illuminate/support": "^8.21 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "BigPictureMedical\\OpenEhr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.57",
        "phpunit/phpunit": "^9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BigPictureMedical\\OpenEhr\\ServiceProvider"
            ]
        }
    }
}
